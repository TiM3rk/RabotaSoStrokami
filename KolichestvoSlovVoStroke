#include <iostream>
#include <string>

using namespace std;

int main() {
    string str;
    getline(cin, str);
    int Answer =  0;
    for (int i = 0; i < str.size(); i++)
    {
        if (str[i] != ' ')
        {
            if (str[i + 1] == ' ' || str[i + 1] == '\0')
            {
                Answer++;
            }
            else{}
        }
        else{}
    }
    cout << Answer;
}
